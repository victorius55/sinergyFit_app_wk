rules_version = '2';

// Force re-deploy on 2024-07-31T00:53:32.484Z
service firebase.storage {
  match /b/{bucket}/o {
    
    // Allow public read access to all files
    match /{allPaths=**} {
      allow read;
    }
    
    // Allow write access only for authenticated users to their own folders
    match /users/{userId}/{allPaths=**} {
      allow write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
